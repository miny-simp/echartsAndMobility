@-webkit-keyframes animations1{
	0% { -webkit-transform:rotate(0deg); }
	3% { -webkit-transform:rotate(-20deg); }
	4% { -webkit-transform:rotate(-20deg); }
	5% { -webkit-transform:rotate(-20deg); }
	6% { -webkit-transform:rotate(0deg); }
