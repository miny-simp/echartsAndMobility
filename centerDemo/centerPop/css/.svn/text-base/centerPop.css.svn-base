/**发光动画字体***/
.animatefont {
    color: #4284B3;
    font-weight: bold;
    -webkit-animation: Glow 0.5s ease infinite alternate;
            animation: Glow 0.5s ease infinite alternate;

}
@-webkit-keyframes Glow {
    from {
        text-shadow: 0 0 5px #fff,
                     0 0 10px #fff,
                     0 0 15px #fff,
                     0 0 20px #00a67c,
                     0 0 35px #00a67c,
                     0 0 40px #00a67c,
                     0 0 50px #00a67c,
                     0 0 75px #00a67c;
    }
    to {
        text-shadow: 0 0 3px #fff,
                     0 0 5px #fff,
                     0 0 7px #fff,
                     0 0 10px #00a67c,
                     0 0 18px #00a67c,
                     0 0 20px #00a67c,
                     0 0 25px #00a67c,
                     0 0 38px #00a67c;
    }
}
@keyframes Glow {
    from {
        text-shadow: 0 0 5px #fff,
                     0 0 10px #fff,
                     0 0 15px #fff,
                     0 0 20px #00a67c,
                     0 0 35px #00a67c,
                     0 0 40px #00a67c,
                     0 0 50px #00a67c,
                     0 0 75px #00a67c;
    }
    to {
        text-shadow: 0 0 3px #fff,
                     0 0 5px #fff,
                     0 0 7px #fff,
                     0 0 10px #00a67c,
                     0 0 18px #00a67c,
                     0 0 20px #00a67c,
                     0 0 25px #00a67c,
                     0 0 38px #00a67c;
    }
}