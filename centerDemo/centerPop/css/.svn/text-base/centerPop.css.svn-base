.center-circle:before{
	width: 1000px; 
    height: 1000px; 
    content:'';
    background: rgba(0,0,0,0.5);
    position:absolute;
    top:0px;
    left:0px;
    border:16px dashed transparent;
    border-right: 16px dashed #50b9eb; 
    border-radius: 50%;
    -moz-transform: rotate(-65deg);
    -webkit-transform: rotate(-65deg);
    transform: rotate(-65deg);
}
.center-circle{
	width: 1000px; 
    height: 1000px; 
    border:16px dashed transparent;
    border-right: 16px solid #50b9eb; 
    border-left:  16px solid #50b9eb; 
    border-radius: 50%;
    /*transform: rotate(-45deg)*/
}
